{
  "extends": "eslint-config-eyeo",
  "root": true,
  "env": {
    "browser": true,
    "webextensions": true
  },
  "globals": {
    "exports": true,
    "ext": true,
    "module": true,
    "require": true
  },
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "curly": ["error", "multi-or-nest", "consistent"]
  }
}
